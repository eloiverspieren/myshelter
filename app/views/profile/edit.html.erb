<div class="banner-profile" style="background: url('https://hd.unsplash.com/photo-1454618454636-a42c7330ccce');">
<div class="col-xs-12 col-sm-3 col-sm-offset-6" id="profile">
    <%= simple_form_for(@profile, :url => profile_path, :method => :put) do |f| %>
    <%= f.error_notification %>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.input :email %>
      <%= f.input :phone %>
    </div>
    <div class="form-actions">
      <%= f.button :submit, class: "btn btn-warning" %>
    </div>
    <% end %>
    <br>

  </div>

</div>
