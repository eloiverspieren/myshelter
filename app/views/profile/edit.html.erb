<div class="banner" style="background: url('https://hd.unsplash.com/photo-1454618454636-a42c7330ccce');">
<div class="container-profile">
    <h1> Editing Profile </h1>
    <%= simple_form_for(@profile, :url => profile_path, :method => :put) do |f| %>
    <%= f.error_notification %>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.input :email %>
      <%= f.input :phone %>
    </div>
    <div class="form-actions">
      <%= f.button :submit, class: "btn btn-success" %>
    </div>
    <% end %>
    <br>
      <%= link_to 'Back to Your Profile', profile_path, class: "btn btn-success" %>

  </div>

</div>
