
<h1>Your Profile</h1>
<%= simple_form_for(@profile) do |f| %>
<%= f.error_notification %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :email %>
  <%= f.input :phone %>
</div>
<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>




