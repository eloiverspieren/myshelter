<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Start day</th>
      <th>End day</th>
      <th>Hikers nb</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.start_day %></td>
        <td><%= booking.end_day %></td>
        <td><%= booking.hikers_nb %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


